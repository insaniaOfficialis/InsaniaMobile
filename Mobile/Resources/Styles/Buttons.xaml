<?xml version="1.0" encoding="UTF-8" ?>
<?xaml-comp compile="true" ?>
<ResourceDictionary 
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml">

    <Style TargetType="Button" x:Key="ButtonPrimary">
        <Setter Property="Background" Value="{StaticResource White1}"/>
        <Setter Property="FontFamily" Value="Georgia"/>
        <Setter Property="FontSize" Value="{OnIdiom Desktop=18, Phone=15}"/>
        <Setter Property="TextColor" Value="{StaticResource Gray1}"/>
    </Style>

</ResourceDictionary>